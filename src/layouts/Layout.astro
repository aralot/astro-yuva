--- 
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>Jawablah beberapa pertanyaan dan dapatkan kelas coding gratis untuk anak Anda</title>
	</head>
	<body>
		<slot />
	</body>
</html>
<style is:global>
  /* Register your custom font family and tell the browser where to find it. */
  @font-face {
    font-family: 'Cera CY';
    src: url('/fonts/Cera-Bold-Web.ttf');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Cera CY';
    src: url('/fonts/Cera-Regular-Web.ttf');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
  }

  html, body {
    height: 100%;
    margin: 0;
    font-family: 'Cera CY', Helvetica, Arial, sans-serif;
  }
</style>
